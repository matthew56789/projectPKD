function calculateHeightForUpselNameDiv() {
    var elements = $('div.upsellSetHeight');
    var setHeight = false;
    elements.each(function () {
        if ($(this).height() > 19) {
            setHeight = true;
        }
    });
    if (setHeight == true) {
        elements.each(function () {
            $(this).css("height", 40);
        });
    }
}

