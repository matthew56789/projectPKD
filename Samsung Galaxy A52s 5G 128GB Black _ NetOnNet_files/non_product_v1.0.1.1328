        if (url && url.indexOf('page=') !== -1) {
            pageParam = url.match(/page=(\d+)&/);
            if (pageParam && pageParam.length >= 1) {
                pageNum = parseInt(pageParam[1], 10);
                if ($.isNumeric(pageNum)) {
                    return pageNum;
