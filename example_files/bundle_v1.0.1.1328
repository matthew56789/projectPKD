    }

    .btn.btn-simple.active {
        color: #ffffff;
        border: 1px solid #0192D5;
        background-color: #0096DB;
        box-shadow: none;
        font-weight: 600;
    }

        .btn.btn-simple.active:hover {
            border-color: #0074A9;
            background-color: #0078AF;
        }

.btn-group .btn.btn-simple {
