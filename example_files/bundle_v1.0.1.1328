@font-face {
    font-family: RebelHand;
    font-weight: bold;
    src: url(/Assets/CSS/Fonts/NetOnNetRebelHand-Bold.otf) format("opentype");
}

/*** Template-specific settings ***/
.box-template-wellpapp .foreground {
    background-image: url(/Assets/Images/Boxes/wellpapp-foreground.png);
}
.box-template-blackfriday .foreground {
    background-image: url(/Assets/Images/Boxes/blackfriday-foreground.png);
}

.box-template-christmas .foreground {
    background-image: url(/Assets/Images/Boxes/christmas-foreground.png);
}

/*** General settings ***/
.box-template {
    width: 100%;
    height: 380px;
    overflow: hidden;
    position: relative;
    user-select: none;
}

.promoSlider .box-template {
    height: 100%;
}

.box-template .foreground {
    background-attachment: scroll;
    background-size: cover;
    background-repeat: no-repeat;
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: 10;
    pointer-events: none;
}

.box-template .innerItem {
    width: 100%;
    height: 100%;
}

.box-template .content {
    display: flex;
    align-items: center;
    width: 100%;
    height: 100%;
    position: absolute;
    margin: auto;
}

    .box-template .content > * {
        display: flex;
        justify-content: center;
        align-items: flex-start;
        flex-basis: 50%;
        flex-shrink: 1;
    }

.box-template .content-inner {
    flex-direction: column;
    flex-shrink: 0;
}

.box-template .product-info {
    margin: 10% 0 0 60px;
    width: 95%;
}

.promoSlider .owl-item .box-template .title,
.box-template .title {
    font: 48px RebelHand;
    text-transform: uppercase;
    color: black;
    text-decoration: none;
}

.promoSlider .owl-item .box-template .subTitle,
.box-template .subTitle {
    color: black;
    font-size: 17px;
    font-weight: 700;
    margin-top: 5px;
    line-height: normal;
    white-space: pre-wrap;
}

.promoSlider .owl-item .box-template .bulletList,
.box-template .bulletList {
    color: black;
    font-size: 17px;
    font-weight: 600;
    margin-top: 15px;
}

    .promoSlider .owl-item .box-template .bulletList li,
    .box-template .bulletList li {
        background-position: 0.1em .45em;
        background-size: 5px;
        background-image: url(/Assets/Images/Boxes/wellpapp-bullet.png);
        background-repeat: no-repeat;
        padding: 0 0 5px 0.8em;
        line-height: 19px;
        margin: 0;
    }

.promoSlider .owl-item .box-template .product-price,
.box-template .product-price {
    display: flex;
    justify-content: center;
    align-items: flex-end;
    margin: 15px 0 0 40px;
}

    .promoSlider .owl-item .box-template .product-price > *,
    .box-template .product-price > * {
        margin: 0;
        flex-basis: auto;
    }

.promoSlider .owl-item .box-template .price-big,
.box-template .price-big {
    color: white;
    background-color: #ed1c24;
    font-size: 45px;
    line-height: normal;
    padding: 0 10px 0 20px;
    font-weight: bold;
    margin: 0;
    margin-right: 15px;
}

.promoSlider .owl-item .box-template .partial-payment,
.box-template .partial-payment {
    color: black;
    font-size: 17px;
    font-weight: 600;
    margin: 0;
    line-height: 13px;
}

    .promoSlider .owl-item .box-template .partial-payment img.card,
    .box-template .partial-payment img.card {
        width: 60px !important;
        display: inline;
        vertical-align: middle;
    }

/*** Product Image */
.promoSlider .owl-item .box-template picture img,
.box-template picture img {
    width: 90% !important;
    padding-bottom: 5%;
    max-width: none;
}

/*** Responsive breakpoints ***/
@media screen and (max-width: 400px) {

    .promoSlider .owl-item .box-template .product-info,
    .box-template .product-info {
        margin-left: 20px;
    }

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 19px;
    }

    .promoSlider .owl-item .box-template .subTitle,
    .box-template .subTitle {
        font-size: 10px;
    }

    .promoSlider .owl-item .box-template .bulletList,
    .box-template .bulletList {
        display: none;
    }

    .promoSlider .owl-item .box-template .product-price,
    .box-template .product-price {
        margin: 5px 0 0 10px;
    }

    .promoSlider .owl-item .box-template .price-big,
    .box-template .price-big {
        font-size: 16px;
        padding: 5px 5px 5px 10px;
    }

    .promoSlider .owl-item .box-template .partial-payment,
    .box-template .partial-payment {
        display: none;
    }
}

@media screen and (min-width: 400px) {

    .promoSlider .owl-item .box-template .product-info,
    .box-template .product-info {
        margin-left: 30px;
    }

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 25px;
    }

    .promoSlider .owl-item .box-template .subTitle,
    .box-template .subTitle {
        font-size: 12px;
    }

    .promoSlider .owl-item .box-template .bulletList,
    .box-template .bulletList {
        display: none;
    }

    .promoSlider .owl-item .box-template .product-price,
    .box-template .product-price {
        margin-left: 20px;
    }

    .promoSlider .owl-item .box-template .price-big,
    .box-template .price-big {
        font-size: 24px;
    }

    .promoSlider .owl-item .box-template .partial-payment,
    .box-template .partial-payment {
        display: none;
    }
}

@media screen and (min-width: 500px) {

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 32px;
    }

    .promoSlider .owl-item .box-template .price-big,
    .box-template .price-big {
        font-size: 25px;
        padding: 0 7px 0 15px;
    }
}

@media screen and (min-width: 600px) {

    .promoSlider .owl-item .box-template .product-info,
    .box-template .product-info {
        margin-left: 45px;
    }

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: clamp(37px, 5.5vw, 42px);
    }

    .promoSlider .owl-item .box-template .subTitle,
    .box-template .subTitle {
        font-size: 17px;
    }

    .promoSlider .owl-item .box-template .bulletList,
    .box-template .bulletList {
        display: none;
    }

    .promoSlider .owl-item .box-template .product-price,
    .box-template .product-price {
        margin-left: 30px;
    }

    .promoSlider .owl-item .box-template .price-big,
    .box-template .price-big {
        font-size: 37px;
    }

    .promoSlider .owl-item .box-template .partial-payment,
    .box-template .partial-payment {
        display: none;
    }
}

@media screen and (min-width: 800px) {
    .promoSlider .owl-item .box-template .product-info,
    .box-template .product-info {
        margin-left: 60px;
    }

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 42px;
    }

    .promoSlider .owl-item .box-template .subTitle,
    .box-template .subTitle {
        font-size: 17px;
    }

    .promoSlider .owl-item .box-template .bulletList,
    .box-template .bulletList {
        display: block;
    }

    .promoSlider .owl-item .box-template .product-price,
    .box-template .product-price {
        margin-left: 40px;
    }

    .promoSlider .owl-item .box-template .price-big,
    .box-template .price-big {
        font-size: 36px;
    }

    .promoSlider .owl-item .box-template .partial-payment,
    .box-template .partial-payment {
        display: inline-block;
    }
}

/* IE Fixes - */
@media screen and (min-width:0\0) and (min-resolution: +72dpi) {

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-family: inherit;
        font-weight: 600;
        text-transform: none;
    }
}

@media screen and (min-width:0\0) and (min-resolution: +72dpi) and (max-width: 400px) {
    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 19px;
        line-height: 19px;
    }
}

@media screen and (min-width:0\0) and (min-resolution: +72dpi) and (min-width: 400px) {
    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 21px;
        line-height: 21px;
    }
}

@media screen and (min-width:0\0) and (min-resolution: +72dpi) and (min-width: 500px) {
    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 23px;
        line-height: 23px;
    }
}

@media screen and (min-width:0\0) and (min-resolution: +72dpi) and (min-width: 600px) {

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 25px;
        line-height: 25px;
    }
}

@media screen and (min-width:0\0) and (min-resolution: +72dpi) and (min-width: 880px) {

    .promoSlider .owl-item .box-template .title,
    .box-template .title {
        font-size: 27px;
        line-height: 27px;
    }
}

.btn {
    font-size: 13px;
    padding: 9px 12px;
    border-radius: 3px;
    outline: none !important;
    -webkit-transition: 0.3s ease background !important;
    -moz-transition: 0.3s ease background !important;
    -ms-transition: 0.3s ease background !important;
    -o-transition: 0.3s ease background !important;
    transition: 0.3s ease background !important;
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    /*-webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);*/
}

    .btn.btn-sm {
        padding: 6px 12px;
    }

    .btn.btn-lg {
        font-size: 16px;
        padding: 10px 12px;
    }

    .btn[disabled].ladda-button {
        opacity: 1;
    }

.form-control {
    height: auto;
    font-size: 13px;
    padding: 9px 12px;
    border-radius: 3px;
    box-shadow: none;
}

    .form-control.small {
        padding: 4px;
        font-size: 12px;
        margin-top: 2px;
    }

textarea.form-control {
    height: 100%;
}

.form-control-inline > [class*='col-']:first-child {
    padding-left: 10px;
}

.form-control-inline > [class*='col-'] {
    padding-left: 1px;
    padding-right: 1px;
}

    .form-control-inline > [class*='col-']:last-child {
        padding-right: 10px;
    }

.form-control:active {
    box-shadow: none;
}

.form-control.input-sm {
    padding: 6px 12px;
}

.form-control.input-lg {
    font-size: 14px;
    padding: 10px 12px;
}

.btn-sm, .btn-group-sm > .btn {
    line-height: initial;
}

.btn-lg, .btn-group-lg > .btn {
    line-height: 20px;
}

.input-sm, .form-horizontal .form-group-sm .form-control {
    line-height: initial;
}

.input-lg, .form-horizontal .form-group-lg .form-control {
    line-height: 20px;
}

.btn-primary {
    color: white;
    font-weight: 600;
    border: 1px solid #0192D5;
    background-color: #0096DB;
}

    .btn-primary:hover, .btn-primary:focus {
        border-color: #0074A9;
        background-color: #0078AF;
    }

.btn-outline-primary {
    color: #0078AF;
    font-weight: 600;
    border: 1px solid #0192D5;
    background-color: #fff;
    padding: 9px 12px;
}

.btn.btn-sm.btn-outline-primary {
    padding: 6px 12px;
}

.btn.btn-lg.btn-outline-primary {
    padding: 10px 12px;
}

.btn-outline-primary:hover, .btn-outline-primary:focus {
    color: #0078AF;
    border-color: #0074A9;
    background-color: #D9EDF7;
}

.btn-primary:focus, .btn-primary:active, .btn-primary:active:focus, .btn-outline-primary:focus, .btn-outline-primary:active, .btn-outline-primary:active:focus {
    outline: 1px;
}

.btn-primary.disabled, .btn-primary[disabled] {
    background-color: #337ab7;
    border-color: #2e6da4;
}

.btn-primary.small, .btn-outline-primary.small {
    padding: 8px;
    padding-top: 5px;
    padding-bottom: 5px;
}

.btn.btn-simple {
